<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>
<?import java.net.URL?>
<GridPane alignment="CENTER" hgap="10.0" prefHeight="800.0" prefWidth="1000.0" styleClass="gridPane" vgap="10.0"
          xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2"
          fx:controller="uk.ac.cam.teamdelta.larry.RunningScreenController">
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
  </columnConstraints>
  <padding>
    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
  </padding>
  <rowConstraints>
    <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
  </rowConstraints>
  <stylesheets>
    <URL value="@ui.css" />
  </stylesheets>
  <BorderPane style="" GridPane.columnIndex="0" GridPane.rowIndex="0">
    <center>
      <ImageView fitHeight="0.0" fitWidth="0.0" pickOnBounds="true" preserveRatio="true">
        <Image url="@images/car.png" />
      </ImageView>
    </center>
    <top>
      <HBox alignment="TOP_CENTER" prefHeight="80.0" prefWidth="200.0" styleClass="HBox">
        <Label alignment="CENTER" ellipsisString="" prefHeight="80.0" prefWidth="350.0" styleClass="titleLabel" text="DriveByAge" textAlignment="CENTER" textFill="WHITE" wrapText="false">
          <font>
            <Font name="ONRAMP" size="35.0" />
          </font>
        </Label>
      </HBox>
    </top>
  </BorderPane>
</GridPane>
