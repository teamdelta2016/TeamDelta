<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>
<?import java.net.URL?>
<GridPane alignment="CENTER" focusTraversable="true" hgap="10.0" styleClass="gridPane" vgap="10.0"
          xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2"
          fx:controller="uk.ac.cam.teamdelta.larry.RunningScreenController">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"/>
    </columnConstraints>
    <BorderPane style="" GridPane.columnIndex="0" GridPane.rowIndex="0">
        <bottom>
            <Button id="beginBtn" fx:id="button" defaultButton="false" ellipsisString="" focusTraversable="false"
                    graphicTextGap="0.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
                    minWidth="-Infinity" mnemonicParsing="false" onAction="#quitGame" prefHeight="-1.0"
                    prefWidth="-1.0" text="quit" underline="false" wrapText="false"
                    BorderPane.alignment="BOTTOM_CENTER">
                <stylesheets>
                    <URL value="@ui.css"/>
                </stylesheets>
                <BorderPane.margin>
                    <Insets bottom="20.0"/>
                </BorderPane.margin>
            </Button>
        </bottom>
        <center>
            <ImageView fx:id="view" fitHeight="0.0" fitWidth="0.0" pickOnBounds="true" preserveRatio="true">
                <Image url="@images/test1out.jpg"/>
            </ImageView>
        </center>
        <top>
            <HBox alignment="TOP_CENTER" prefHeight="80.0" prefWidth="200.0" styleClass="HBox">
                <Label alignment="CENTER" ellipsisString="" prefHeight="80.0" prefWidth="350.0" styleClass="titleLabel"
                       text="DriveByAge" textAlignment="CENTER" textFill="WHITE" wrapText="false">
                    <font>
                        <Font name="ONRAMP" size="35.0"/>
                    </font>
                </Label>
            </HBox>
        </top>
    </BorderPane>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
    <rowConstraints>
        <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
    </rowConstraints>
    <stylesheets>
        <URL value="@ui.css"/>
    </stylesheets>
</GridPane>
