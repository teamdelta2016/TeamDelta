<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>
<?import java.net.URL?>
<GridPane alignment="CENTER" gridLinesVisible="false" hgap="10.0" styleClass="gridPane" vgap="10.0"
          xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2"
          fx:controller="uk.ac.cam.teamdelta.larry.LocationConfirmScreenController">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"/>
    </columnConstraints>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
    <rowConstraints>
        <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
    </rowConstraints>
    <stylesheets>
        <URL value="@ui.css"/>
    </stylesheets>
    <BorderPane style="&#10;" GridPane.columnIndex="0" GridPane.rowIndex="0">
        <center>
            <GridPane>
                <columnConstraints>
                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                </columnConstraints>
                <padding>
                    <Insets left="5.0" right="5.0"/>
                </padding>
                <rowConstraints>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                </rowConstraints>
                <Label id="errorText" ellipsisString="" prefWidth="-1.0" text="Is this correct?" textAlignment="CENTER"
                       wrapText="true" GridPane.columnIndex="0" GridPane.halignment="CENTER" GridPane.rowIndex="0">
                    <font>
                        <Font name="Dax-Regular" size="15.0" fx:id="x1"/>
                    </font>
                </Label>
                <Label id="errorText" fx:id="retrievedLocation" ellipsisString="" font="$x1" prefWidth="-1.0" text=""
                       textAlignment="CENTER" wrapText="true" GridPane.columnIndex="0" GridPane.halignment="CENTER"
                       GridPane.rowIndex="1"/>
                <GridPane alignment="CENTER" GridPane.columnIndex="0" GridPane.rowIndex="2">
                    <children>
                        <Button onAction="#handleReject" text="no" GridPane.columnIndex="0" GridPane.halignment="CENTER"
                                GridPane.rowIndex="0"/>
                        <Button defaultButton="true" onAction="#handleConfirm" text="yes" GridPane.columnIndex="1"
                                GridPane.halignment="CENTER" GridPane.rowIndex="0"/>
                    </children>
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                    </columnConstraints>
                    <padding>
                        <Insets top="50.0"/>
                    </padding>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    </rowConstraints>
                </GridPane>
            </GridPane>
        </center>
        <top>
            <HBox alignment="TOP_CENTER" prefHeight="80.0" prefWidth="200.0" style="&#10;" styleClass="HBox">
                <Label alignment="CENTER" minHeight="26.0" prefHeight="80.0" prefWidth="350.0" styleClass="titleLabel"
                       text="location" textAlignment="CENTER" textFill="WHITE" wrapText="false">
                    <font>
                        <Font name="MuseoSlab-500" size="50.0"/>
                    </font>
                </Label>
            </HBox>
        </top>
    </BorderPane>
</GridPane>
